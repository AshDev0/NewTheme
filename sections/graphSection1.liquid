<section id="graph-section">
  <div class="graph-container">
    {% for block in section.blocks %}
      <div class="graph-items">
        <h2>{{ block.settings.heading }}</h2>
        <p>{{ block.settings.desc }}</p>
      </div>
    {% endfor %}
  </div>
</section>
{% schema %}
{
  "name": "Graph Section",
  "tag": "section",
  "class": "section",
  "settings": [
  ],
  "blocks": [
      {
      "name": "block",
        "type": "block",
        "settings": [
          {
              "type": "text",
              "id": "heading",
              "label": "Heading"
          },
          {
              "type": "richtext",
              "id": "desc",
              "label": "Description"
          }
      ]
  }
  ],
  "presets": [
    {
      "name": "Graph Section",
      "settings": {
      }
    }
  ]
}
{% endschema %}
