<section id="bookCall">
<div class="bookCall-container">
<div class="bookCall-content">
    <h2>{{section.settings.heading}}</h2>
    <a href="#">{{section.settings.bookCall}}</a>
</div>
</div>
</section>
{% schema %}
  {
    "name": "Book Section",
    "tag": "section",
    "class": "section",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Main Title"
    },
    {
        "type": "text",
        "id": "bookCall",
        "label": "Button Title"
    },
    {
        "type": "url",
        "id": "url",
        "label": "Button Url"
    }
    ],
    "presets": [
      {
        "name": "Book Section",
        "settings": {
        }
      }
    ]
  }
{% endschema %}